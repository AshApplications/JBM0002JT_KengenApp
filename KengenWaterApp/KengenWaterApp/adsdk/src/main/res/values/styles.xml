        <item name="colorControlActivated">@color/colorBlack</item>
        <item name="colorControlNormal">@color/colorBlack</item>
    </style>

    <style name="fontNormal">
        <item name="fontFamily">@font/mfont</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="fontMedium">
        <item name="fontFamily">@font/mfont_medium</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="fontSemibold">
        <item name="fontFamily">@font/mfont_semibold</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="fontBold">
        <item name="fontFamily">@font/mfont_bold</item>
        <item name="android:includeFontPadding">false</item>
    </style>


</resources>